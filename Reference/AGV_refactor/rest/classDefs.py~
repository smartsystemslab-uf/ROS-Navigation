class Bot:
	def __init__(self, endPointAddress, botType, status):
		self.endPointAddress = endPointAddress
		self.botType = botType
		self.status = status

	def getAddress(self):
		return self.endPointAddress

	def getBotType(self):
		return self.botType

	def setStatus(self, status):
		self.status = status

	def getStatus(self):
		return self.status

class Host:
	def __init__(self, hostAddress):
		self.hostAddress = hostAddress

	def setAddress(self, hostAddress):
		self.hostAddress = hostAddress

	def getAddress(self):
		return self.hostAddress
	
